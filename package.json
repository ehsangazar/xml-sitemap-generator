{
  "name": "@ehsangazar/xml-sitemap-generator",
  "version": "0.0.0-semantically-released",
  "description": "Easily create XML sitemaps for your website.",
  "homepage": "https://github.com/ehsangazar/xml-sitemap-generator",
  "author": "Ehsan Gazar",
  "keywords": [
    "sitemap",
    "xml"
  ],
  "type": "module",
  "main": "./src/generator.ts",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm ./generator.ts",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "@types/jest": "^29.5.12",
    "async": "3.2.5",
    "cheerio": "1.0.0-rc.12",
    "cp-file": "10.0.0",
    "crawlee": "^3.9.1",
    "crypto-random-string": "5.0.0",
    "date-fns": "3.6.0",
    "lodash": "4.17.21",
    "mitt": "3.0.1",
    "normalize-url": "8.0.1",
    "simplecrawler": "1.1.9",
    "url-parse": "1.5.10"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "eslint": "9.0.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehsangazar/xml-sitemap-generator.git"
  },
  "bugs": {
    "url": "https://github.com/ehsangazar/xml-sitemap-generator/issues"
  }
}
